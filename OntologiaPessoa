class Pessoa {//Conceito geral
	String nome;
}
class Professor extends Pessoa {//Conceito específico do domínio educacional
	String disciplina;
}
class Turma {//Conceito que representa uma entidade educacional com relações
	String codigo;
	Professor professorResponsavel;
	Pessoa aluno;
	public Turma(String codigo, Professor professorResponsavel, Pessoa aluno) {
		this.codigo = codigo;
		this.professorResponsavel = professorResponsavel;
		this.aluno = aluno;
	}
	void exibirRelacoes() {
		System.out.println("Turma: " + codigo);
		System.out.println("Professor responsável: " + professorResponsavel.nome + " | Disciplina: "
				+ professorResponsavel.disciplina);
		System.out.println("Aluno matriculado: " + aluno.nome);
	}
}
public class Ontologia {
	public static void main(String[] args) {
		Professor prof = new Professor();
		prof.nome = "Professor João";
		prof.disciplina = "Inteligência Artificial";
		Pessoa aluno = new Pessoa();
		aluno.nome = "Maria Vitória";
		Turma turma = new Turma("IA-Miguel", prof, aluno);
		turma.exibirRelacoes();
	}
}

